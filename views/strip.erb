<div id="title">#<%=@strip.id%> &ndash; <%= @strip.title %></div>


<div id="Strip"> <img src="<%=@strip.filename%>"></div>

<div id="nav">
     <a href="<%= @first.id %>">First</a>
     <% if @strip.id == 1 %>
     	<a href="/1">Prev</a>
     <% else %>
          <a href="/<%= @prev.id%> ">Prev</a> 
     <% end %>
     <% if @strip.id == @last.id %>
          <a href="/">Next</a>
     <% else %>
     	<a href="/<%= @next.id %>">Next</a>
     <%end%>
     <a href="/">Last</a>
</div>
<div id="blurb"><span class="posted">Posted on <%= @strip.posted %></span></div>



